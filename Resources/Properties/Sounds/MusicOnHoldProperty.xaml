<UserControl x:Class="IntegraDesigner.Resources.Properties.MusicOnHoldProperty"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="auto" Width="auto">
    <StackPanel>
        <StackPanel Orientation="Horizontal">
            <Label Canvas.Left="0" Canvas.Top="30" Height="25" Name="lblclase" Width="87" >Class Name:</Label>
            <ComboBox Height="23" Name="cmbclass" Width="168" IsEditable="True" />
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <Label Canvas.Left="0" Canvas.Top="30" Height="25" Name="lblduration" Width="87" >Duration:</Label>
            <TextBox Canvas.Left="79" Canvas.Top="59" Height="23" Name="txtduration" Width="167"  />
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <TextBlock Text="Description:" Width="76"></TextBlock>
            <TextBox x:Name="txtdesc" Width="624" Margin="10,0,0,0"></TextBox>
        </StackPanel>
    </StackPanel>
</UserControl>
