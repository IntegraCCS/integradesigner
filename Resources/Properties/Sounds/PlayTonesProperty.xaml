<UserControl x:Class="IntegraDesigner.Resources.Properties.PlayTonesProperty"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="auto" Width="auto">
    <StackPanel>
        <StackPanel Orientation="Horizontal">
            <Label Canvas.Left="0" Canvas.Top="30" Height="25" Name="lbltone" Width="87" >Tone Name:</Label>
            <ComboBox Height="23" Name="cmbtone" Width="120" IsEditable="True">
                <ComboBoxItem>dial</ComboBoxItem>
                <ComboBoxItem>busy</ComboBoxItem>
                <ComboBoxItem>ring</ComboBoxItem>
                <ComboBoxItem>congestion</ComboBoxItem>
                <ComboBoxItem>callwaiting</ComboBoxItem>
                <ComboBoxItem>dialrecall</ComboBoxItem>
                <ComboBoxItem>record</ComboBoxItem>
                <ComboBoxItem>info</ComboBoxItem>
                <ComboBoxItem>name</ComboBoxItem>
            </ComboBox>
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <TextBlock Text="Description:" Width="76"></TextBlock>
            <TextBox x:Name="txtdesc" Width="624" Margin="10,0,0,0"></TextBox>
        </StackPanel>

    </StackPanel>
</UserControl>
