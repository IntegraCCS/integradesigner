<UserControl x:Class="IntegraDesigner.Resources.Properties.ARRAYProperty"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="auto" Width="auto">
    <StackPanel>
        <StackPanel Orientation="Horizontal">
            <Label Canvas.Left="0" Canvas.Top="30" Height="25" Name="lblvar" Width="205">Variable Names (comma delimited):</Label>
            <TextBox Canvas.Left="79" Canvas.Top="59" Height="23" Name="txtvars" Width="438" />
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <Label Canvas.Left="0" Canvas.Top="30" Height="25" Name="lblval" Width="205">Values (comma delimited):</Label>
            <TextBox Canvas.Left="79" Canvas.Top="59" Height="23" Name="txtvals" Width="437"  />
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <TextBlock Text="Description:" Width="197"></TextBlock>
            <TextBox x:Name="txtdesc" Width="624" Margin="10,0,0,0"></TextBox>
        </StackPanel>

    </StackPanel>
</UserControl>
