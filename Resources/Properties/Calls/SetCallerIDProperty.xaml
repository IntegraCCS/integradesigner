<UserControl x:Class="IntegraDesigner.Resources.Properties.SetCallerIDProperty"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="auto" Width="auto">
    <StackPanel>
        
        <StackPanel Orientation="Horizontal">
            <Label Canvas.Left="0" Canvas.Top="30" Height="25" Name="lblprop" Width="90"  >Property:</Label>
            <ComboBox Canvas.Left="79" Canvas.Top="59" Height="23" Name="txtprop" Width="112" >
                <ComboBoxItem>num</ComboBoxItem>
                <ComboBoxItem>all</ComboBoxItem>
                <ComboBoxItem>name</ComboBoxItem>
                <ComboBoxItem>ANI</ComboBoxItem>
                <ComboBoxItem>DNID</ComboBoxItem>
                <ComboBoxItem>RDNIS</ComboBoxItem>
                
            </ComboBox>
        </StackPanel>
        
        <StackPanel Orientation="Horizontal">
            <Label Canvas.Left="0" Canvas.Top="30" Height="25" Name="lblval" Width="89" >Value:</Label>
            <TextBox Canvas.Left="79" Canvas.Top="59" Height="23" Name="txtval" Width="200"  />
        </StackPanel>

        <StackPanel Orientation="Horizontal">
            <TextBlock Text="Description:" Width="81"></TextBlock>
            <TextBox x:Name="txtdesc" Width="624" Margin="10,0,0,0"></TextBox>
        </StackPanel>

    </StackPanel>
</UserControl>
