<UserControl x:Class="IntegraDesigner.Resources.Properties.PickupProperty"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
   >
    <StackPanel>

        <StackPanel Orientation="Horizontal">
            <Label Canvas.Left="0" Canvas.Top="30" Height="25" Name="lblexten" Width="73">Channels:</Label>
            <TextBox Canvas.Left="79" Canvas.Top="59" Height="23" Name="txtexten" Width="619" ToolTip="Use ampersand to concatenate channels SIP/1001 etc"></TextBox>
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <TextBlock Text="Description:"></TextBlock>
            <TextBox x:Name="txtdesc" Width="624" Margin="10,0,0,0"></TextBox>
        </StackPanel>

    </StackPanel>
</UserControl>
