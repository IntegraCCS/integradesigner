<UserControl x:Class="IntegraDesigner.Resources.Properties.VOLUMEProperty"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="auto" Width="auto">
    <StackPanel>
        <StackPanel Orientation="Horizontal">
            <Label Canvas.Left="0" Canvas.Top="30" Height="25" Name="lbltype" Width="77"  >Type:</Label>
            <ComboBox Canvas.Left="79" Canvas.Top="59" Height="23" Name="txttype" Width="46" >
                <ComboBoxItem>TX</ComboBoxItem>
                <ComboBoxItem>RX</ComboBoxItem>
            </ComboBox>
        </StackPanel>

        <StackPanel Orientation="Horizontal">
            <Label Canvas.Left="0" Canvas.Top="30" Height="25" Name="lblval" Width="80" >Value:</Label>
            <TextBox Canvas.Left="79" Canvas.Top="59" Height="23" Name="txtval" Width="44"  />
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <TextBlock Text="Description:" Width="68"></TextBlock>
            <TextBox x:Name="txtdesc" Width="624" Margin="10,0,0,0"></TextBox>
        </StackPanel>


    </StackPanel>
</UserControl>
