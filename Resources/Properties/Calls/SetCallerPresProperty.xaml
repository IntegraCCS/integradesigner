<UserControl x:Class="IntegraDesigner.Resources.Properties.SetCallerPresProperty"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="auto" Width="auto">
    <StackPanel>
        <StackPanel Orientation="Horizontal">
            <Label Canvas.Left="0" Canvas.Top="30" Height="25" Name="lblcaller" Width="116"  >Caller Presentation:</Label>
            <ComboBox Canvas.Left="79" Canvas.Top="59" Height="23" Name="txtcaller" Width="158" >
                <ComboBoxItem>allowed</ComboBoxItem>
                <ComboBoxItem>prohib</ComboBoxItem>
                <ComboBoxItem>allowed_not_screened</ComboBoxItem>
                <ComboBoxItem>allowed_passed_screen</ComboBoxItem>
                <ComboBoxItem>allowed_failed_screen</ComboBoxItem>
                <ComboBoxItem>prohib_not_screened</ComboBoxItem>
                <ComboBoxItem>prohib_passed_screen</ComboBoxItem>
                <ComboBoxItem>prohib_failed_screen</ComboBoxItem>
                <ComboBoxItem>unavailable</ComboBoxItem>
            </ComboBox>
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <TextBlock Text="Description:" Width="106"></TextBlock>
            <TextBox x:Name="txtdesc" Width="624" Margin="10,0,0,0"></TextBox>
        </StackPanel>


    </StackPanel>
</UserControl>
