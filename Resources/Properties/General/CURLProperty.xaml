<UserControl x:Class="IntegraDesigner.Resources.Properties.CURLProperty"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="auto" Width="auto">
    <StackPanel>
        <StackPanel Orientation="Horizontal">
            <Label Canvas.Left="0" Canvas.Top="30" Height="25" Name="lblurl" Width="90">URL:</Label>
            <TextBox Canvas.Left="79" Canvas.Top="59" Height="23" Name="txturl" Width="330"  />
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <Label Canvas.Left="0" Canvas.Top="30" Height="25" Name="lblpost" Width="90">Post Data:</Label>
            <TextBox Canvas.Left="79" Canvas.Top="59" Height="23" Name="txtpost" Width="331"  />
            <CheckBox x:Name="chkpost" Content="Is POST?"></CheckBox>
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <Label Canvas.Left="0" Canvas.Top="30" Height="25" Name="lblresult" Width="90">Result Variable:</Label>
            <TextBox Canvas.Left="79" Canvas.Top="59" Height="23" Name="txtresult" Width="331"  />
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <TextBlock Text="Description:" Width="82"></TextBlock>
            <TextBox x:Name="txtdesc" Width="624" Margin="10,0,0,0"></TextBox>
        </StackPanel>



    </StackPanel>
</UserControl>
