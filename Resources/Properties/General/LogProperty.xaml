<UserControl x:Class="IntegraDesigner.Resources.Properties.LogProperty"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="auto" Width="auto">
    <StackPanel>

        <StackPanel Orientation="Horizontal">
            <Label Canvas.Left="0" Canvas.Top="30" Height="25" Name="lbltext" Width="70" >Log Level:</Label>
            <ComboBox Canvas.Left="79" Canvas.Top="59" Height="23" Name="txtlevel" Width="158" >
                <ComboBoxItem>ERROR</ComboBoxItem>
                <ComboBoxItem>WARNING</ComboBoxItem>
                <ComboBoxItem>NOTICE</ComboBoxItem>
                <ComboBoxItem>DEBUG</ComboBoxItem>
                <ComboBoxItem>VERBOSE</ComboBoxItem>
                <ComboBoxItem>DTMF</ComboBoxItem>
                
            </ComboBox>
        </StackPanel>

        <StackPanel Orientation="Horizontal">
            <Label Canvas.Left="0" Canvas.Top="30" Height="25" Name="lblkey" Width="70" >Message:</Label>
            <TextBox Canvas.Left="79" Canvas.Top="59" Height="23" Name="txtmessage" Width="500"  />
        </StackPanel>

        <StackPanel Orientation="Horizontal">
            <TextBlock Text="Description:"></TextBlock>
            <TextBox x:Name="txtdesc" Width="624" Margin="10,0,0,0"></TextBox>
        </StackPanel>

    </StackPanel>
</UserControl>
